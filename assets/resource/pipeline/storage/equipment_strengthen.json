{
    "=装备强化总流程":{
        "recognition": "OCR",
        "expected":["研发"],
        "next":["装备强化"]
    },
    "装备强化":{
        "recognition": "OCR",
        "expected":["装备强化"],
        "action": "Click",
        "next":["选择强化的装备"]
    },
    "选择强化的装备":{
        "recognition": "TemplateMatch",
        "template": "storage/slct_equip.png",
        "action": "Click",
        "target": [215,310,10,40],
        "post_delay": 1500,
        "next":["选择高级装备"]
    },
    "选择高级装备":{
        "recognition": "TemplateMatch",
        "template": [
            "storage/g_equip2.png",
            "storage/g_equip1.png"
        ],
	"post_delay": 1000,
	"pre_delay": 1000,
        "threshold":0.9,
        "action": "Click",
        "next":["选择低级装备","选择高级装备"]
    },
    "选择低级装备":{
        "recognition": "TemplateMatch",
        "template": "storage/stdby.png",
        "action": "Click",
        "next":["强制点击至无智能选择"]
    },
    "强制点击至无智能选择":{
        "recognition": "OCR",
        "expected":["智能选择"],
        "action": "Click",
