            "强制点击至无智能选择",
            "装备强化选择确定"
        ]
    },
    "装备强化选择确定":{
        "recognition": "OCR",
        "expected":["确定"],
        "action": "Click",
        "next":[
            "装备强化确定"
        ]
    },
    "装备强化确定":{
        "recognition": "OCR",
        "expected":["确定"],
        "action": "Click",
        "next":[
            "装备强化最终确定"
        ]
    },
    "装备强化最终确定":{
        "recognition": "OCR",
        "expected":["确定"],
        "action": "Click",
        "roi" : [550,500,800,200],
        "post_delay": 1000,
        "next":["装备强化最终确定","进入导航栏"]
    },
    "进入导航栏":{
        "recognition": "TemplateMatch",
